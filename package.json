{
  "name": "webcodebook",
  "version": "1.3.1",
  "description": "Interactive data set summaries",
  "keywords": [
    "data",
    "summary",
    "interactive",
    "codebook"
  ],
  "homepage": "https://github.com/rhoinc/web-codebook",
  "license": "MIT",
  "author": "Rho, Inc.",
  "main": "build/webcodebook.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhoinc/web-codebook.git"
  },
  "scripts": {
    "build": "npm run bundle && npm run format",
    "bundle": "rollup -c",
    "format": "npm run format-src && npm run format-build",
        "format-build": "prettier --single-quote --write ./build/webcodebook.js",
        "format-src": "prettier --single-quote --write \"./src/**/*.js\"",
    "watch": "rollup -c -w"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-watch": "~3.2.2",
    "prettier":"1.4.4"
  },
  "dependencies": {
    "d3": "~3",
    "webcharts": "~1.7"
  },
  "bugs": {
    "url": "https://github.com/rhoinc/web-codebook/issues"
  }
}
